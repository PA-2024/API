@model IEnumerable<API_GesSIgn.Models.Error>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Error_Funtion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Error_DateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Error_Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Error_Solved)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Error_Funtion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Error_DateTime)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Error_Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Error_Solved)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Error_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Error_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Error_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
